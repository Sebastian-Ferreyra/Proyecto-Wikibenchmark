<?php

namespace Tablas\Tabla;

/**
* @Entity
*
**/

class Juego {

    /**
    * @var int
    *   /**
    * @Id
    * @Column(type="integer")
    * @GeneratedValue
    * */
    private $id;

    /**
    * @var string
    * @Column(length=150)
    */
    private $nombre;

    /**
    * @var string
    * @Column(type="text")
    */
    private $requerimientos;

    
       /**
     * Constructor
     */
    public function __construct()
    {
        $this->Benchmark = new \Doctrine\Common\Collections\ArrayCollection();
    }

    
}
